@using System.Linq;
@using SinglePageTest.Models;
@using System.Collections.Generic;

@model dynamic
@{
    var items = (Model.items == null || Model.items.Length == 0)
        ? new[] { new IndexItemModel() }
        : Model.items;
}

<section>
    <ul data-bind="foreach: items">
        @foreach(var item in items)
        {
        <li data-bind="text: name">@item.name</li>
        }
    </ul>
</section>
